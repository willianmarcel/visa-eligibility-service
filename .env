# Configurações do Banco de Dados
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/visadev"
USE_SQLITE_FALLBACK=false

# Configurações do Redis
REDIS_HOST=localhost
REDIS_PORT=6379
